{# layout.twig #}
<!DOCTYPE html>
<html lang="en">
<head>
	<!--[if IE 6]>		<html id="section-reco_main" class="nojs ie6 ieb7 ieb8 ieb9 ieb10 split1 nosplit5 platform-PC platform-notouch"><![endif]-->
	<!--[if IE 7]>		<html id="section-reco_main" class="nojs iea6 ie7 ieb8 ieb9 ieb10 split1 nosplit5 platform-PC platform-notouch"><![endif]-->
	<!--[if IE 8]>		<html id="section-reco_main" class="nojs iea6 iea7 ie8 ieb9 ieb10 split1 nosplit5 platform-PC platform-notouch"><![endif]-->
	<!--[if IE 9]>		<html id="section-reco_main" class="nojs iea6 iea7 iea8 ie9 ieb10 split1 nosplit5 platform-PC platform-notouch"><![endif]-->
	<meta name="viewport" content="width=device-width">
	<meta name="MobileOptimized" content="320"/>
	<meta name="HandheldFriendly" content="true"/>
	<meta charset="utf-8">
	<meta name="format-detection" content="telephone=no">
	<meta name="viewport min-width=375px" content="min-width=375, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
	<link rel="stylesheet" type="text/css" href="css/style.css">
	<!--[if lt IE 9]>
		<script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/r29/html5.min.js"></script>
	<![endif]-->
	<script src="js/main.min.js"></script>
	<title>Инвестиции в IPO — ITI Capital</title>
	<!-- <meta name="description" content="ITI Capital собирает заявки клиентов на участие в IPO Saudi Aramco. Заработайте на крупнейшем IPO в мире!"> -->
	<meta property="og:url" content="https://iticapital.ru/ipo/">
	<meta property="og:type" content="website">
	<meta property="og:title" content="Инвестиции в IPO — ITI Capital">
	<!-- <meta property="og:image" content="img/assets/OpenGraph-Aramco.png"> -->
	<!-- <meta property="og:description" content="ITI Capital собирает заявки клиентов на участие в IPO Saudi Aramco. Заработайте на крупнейшем IPO в мире!"> -->
	<meta property="og:site_name" content="ITI Capital">
	<meta property="og:locale" content="ru_RU">
</head>
<body>
	{% include "../blocks/header.twig" %}
	<main class="wrapper">
		<div class="news-layout">
			<div class="news-layout-header {{ headerModifier }}">
				<div class="news-layout-header__inner">
					{% if headerModifier != 'news-layout-header--career' %}
						<a href="#" class="news-layout-header__back">
							<- back
						</a>
					{% endif %}

					<div class="news-layout-header__text">
						<h1 class="news-layout-header__title">
							{{title}}
						</h1>
						{% if headerModifier == 'news-layout-header--big' %}
							<div class="news-layout-header__date">12 March</div>
						{% endif %}
					</div>

					<div class="news-layout-header__image">
						{% if headerModifier == 'news-layout-header--big' %}
							<img src="./img/news-header-placeholder.jpg" alt="">
						{% endif %}
					</div>
					{% if headerModifier == 'news-layout-header--search' %}
						<a href="#" class="news-layout-header__search">
							<img src="./img/search.svg" alt="search">
						</a>
					{% endif %}
				</div>

				{% if nav == 'true' %}
					<nav class="news-nav">
						<ul class="news-nav__list">
							<li>
								<a href="#" class="active">
									All News
								</a>
							</li>

							<li>
								<a href="#">
									Research and Analysis
								</a>
							</li>

							<li>
								<a href="#">
									Company News
								</a>
							</li>
						</ul>
					</nav>
				{% endif %}
			</div>

			{% block page %}{% endblock %}
		</div>
	</main>

	<footer class="footer footer--short">
		<div class="footer__index-disclaimer">
			<div class="g-typography">
				<p>
					ITI Capital Ltd is a registered company in England and Wales (registration no. 02926252), authorised and regulated in the UK by the FCA (registration no. 1z71487), and is a member of the London Stock Exchange.
				</p>

				<p>
					Registered company in England and Wales Registration Number: 02926252 VAT No: 904 4506 46. FCA Registration No. 171487 ITI Capital Limited Level 33 Tower 42, 25 Old Broad Street, London EC2N 1HQ, UK
				</p>
			</div>
		</div>

		<div class="footer__copyright">
			© iticapital.com, 1994—2020
		</div>
	</footer>

	<div class="_overlay overlay" onclick="o2.popups.closePopup()"></div>
	{% include "../blocks/popup-alert-covid.twig" %}
	{% include "../blocks/popup-alert-svs-securities.twig" %}
	{# {% include "../blocks/popup-alert-fraud.twig" %} #}
	{% include "../blocks/cookie.twig" %}
</body>
</html>